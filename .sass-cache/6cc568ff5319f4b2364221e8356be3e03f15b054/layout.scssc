:@template"È&/**
 * SASS/SCSS/Compass Transcription of Skeleton Responsive Grid
 * originally authored by Dave Gamache. This translation offers
 * mixins that support fluid and as well as fixed grids to supplement
 * the work that Dave Gamache has done.
 * @author Dennis Thompson <http://www.atomicpages.net>
 * @copyright Copyright (c) 2013 AtomicPages LLC
 * @license MIT
 * @version 1.6.1
 */

// IMPORTS
@import "vars"; // import _global_variables.scss
@import "mixins"; // import _mixins.scss
@import "compass/css3";

/* TYPOGRAPHY (Baseline Corrections)
================================================== */
h1{ line-height: 49px; margin-bottom: 14px;}
h2{ line-height: 42px; margin-bottom: 7px;}
h3{ line-height: 35px; margin-bottom: 7px;}
h4{ line-height: 28px; margin-bottom: 7px;}
h5{ line-height: 21px;}

p{ margin: 0 0 21px 0;}
p.lead{ line-height: 28px;}


/* BLOCKQUOTES (Baseline Corrections)
================================================== */
blockquote, blockquote p{line-height: 21px;}
blockquote {margin: 0 0 21px; padding: 7px 28px 0 19px;}

hr{ margin: 14px 0 28px;}

/* LISTS (Baseline Corrections)
================================================== */
ul, ol {margin-bottom: 21px;}
li { line-height: 21px; margin-bottom: 14px;}

.row{   margin-bottom: 21px; }

/* BUTTONS (Baseline Corrections)
=================================================== */
input[type="button"],input[type="submit"]{
	font-size: 14px;
	font-weight: 300;
	width: 100%;
	color: white;
    background: none;
    border: none;
    text-shadow: none !important;
    @include border-radius(7px);
    @include transition-duration(.4s);
    -webkit-appearance: none;
}
input[type="button"]:hover, input[type="button"]:active, input[type="submit"]:hover{
	font-weight: 300;
    background: none;
    border: none;
    color: white;
    text-shadow: none !important;
    -webkit-appearance: none;

}


/* SITE STYLES
 * ------------------------------------------------ */
/* Add site styles here */
/* For example: a { color: #000; } */
	

body, html, main{	height: 100%; }
html{
	overflow-x:hidden;
}

.left{ float: left; }
.right{ float: right; }

.arrow{
	width: 100%;
	z-index: 5;

	i{
		position: relative;
		left: 50%;
		margin-left: -12px;
		@include transition-duration(.4s);
	}
	i:hover{
		color: #1e1e1e;
	}

}

p{
	color: #ddd !important;
	font-weight: normal;
}

h1{ 
	font-weight: 700;
	margin-bottom: 0;
	line-height: 77px;
	font-size: 76px;
}

h2{
	font-weight: 300;
	line-height: 49px;
	font-size: 48px;
}

h3{
	// display: inline-block;
	border-bottom: 1px solid white;
}

.landing, .about, .media, .contact{
	position: relative;
	margin: 0;
	padding: 0;
	width: 100%;
	
	*, *:hover{
		color: white;
	}
}

.landing{
	height: 100%;
	@include box-sizing(border-box);
	padding: 56px 0px;
	// background: linear-gradient(0deg, rgba(82,54,69,.6), rgba(82,54,69,.6)), url("../images/bg.jpg") no-repeat center center fixed;
	background: rgba(82,54,69,.7);
	
	video{
		position: fixed; right: 0; bottom: 0;
		min-width: 100%; min-height: 100%;
		width: auto; height: auto; z-index: -100;
		@include background-size(cover);
		overflow: hidden;
	}
	.container{
		top: 25%;
	}

	header{
		margin-top: 42px;
	}

 	.action{
 		margin-top: 63px;
 		text-align: center;
 	}

 	h4{
 		display: inline-block;
 		margin-left: 7px;
 		margin-right: 7px;
    	font-weight: 400;
        font-size: 14px;
    	text-align: center;
 	}

    a{
    	color: white;
        text-decoration: none;
        padding: 14px 0;
        width: 266px;
        cursor: pointer;
        display: inline-block;
        background-color: #22ad84;
        @include border-radius(7px);
        @include transition-duration(.4s);
        border-bottom: 3px solid #1B7E61;
    }
    a:hover{
        background-color: #1e1e1e;
        border-bottom: 3px solid #1e1e1e;
    }

    i{
    	margin-left: 7px;
    }
}

.about{
	padding: 56px 0px;
	background-color: #22ad84;

	header{
		margin-bottom: 35px;
	}

	article{
		p{
			margin-bottom: 14px;
		}
		a{
			text-decoration: none;
			color: #1e1e1e;
			
			i{
				margin-left: 7px;
				color: #1e1e1e;
			}
		}
		a:hover{
			text-decoration: underline;
			color: #1e1e1e;

			i{
				color: #1e1e1e;
				text-decoration: underline;
			}
		}
	}
	
	ul{
		margin-top: 14px;
		margin-left: auto;
		margin-right: auto;
		text-align: center;
	}
	
	li{
		width: 133px;
		height: 133px;
		display: inline-block;
		margin: 0 49px; 
		border-radius: 50%;
		background-color: #1B7E61;
		
		a{
			display: block;
			height: 133px;
		}

		i{
			margin-top: 23%;
		}
	}
	li:hover{
		background-color: #1e1e1e;
	}
}

.media{
	max-height: 700px;
	min-height: 532px;
	height: 75%;
	background-color: #E84C3D;

	.gradient{
		position: absolute;
		background: -moz-linear-gradient(top,  rgba(0,0,0,1) 0%, rgba(3,3,3,1) 1%, rgba(255,255,255,0) 100%);
		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(0,0,0,1)), color-stop(1%,rgba(3,3,3,1)), color-stop(100%,rgba(255,255,255,0)));
		background: -webkit-linear-gradient(top,  rgba(0,0,0,1) 0%,rgba(3,3,3,1) 1%,rgba(255,255,255,0) 100%);
		background: -o-linear-gradient(top,  rgba(0,0,0,1) 0%,rgba(3,3,3,1) 1%,rgba(255,255,255,0) 100%);
		background: -ms-linear-gradient(top,  rgba(0,0,0,1) 0%,rgba(3,3,3,1) 1%,rgba(255,255,255,0) 100%);
		background: linear-gradient(to bottom,  rgba(0,0,0,1) 0%,rgba(3,3,3,1) 1%,rgba(255,255,255,0) 100%);
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#00ffffff',GradientType=0 );

		width: 100%;
		height: 210px;
		z-index: 4;
	}

	header{
		position: absolute;
		margin-bottom: -210px;
		z-index: 5;

		h1{
			padding-top: 56px;
		}
	}

	.arrow{
		position: absolute;
		margin-top: -49px;
		z-index: 5;
	}

	.desc{
		top: 75%;
	}
	p{
		@include box-sizing(border-box);
		padding-left: 7px;
		background-color: rgb(82, 54, 69);
	}

	// .slides-pagination, .prev, .next{
	// 	padding: 7px;
	// }

	.slides-navigation a{
		padding: 0 14px;
	}
}

.contact{
	padding: 56px 0px;
	@include box-sizing(border-box);
	background-color: #34495E;

	header{
		margin-bottom: 35px;
	}

	article{
		img{
			display: block;
			@include border-radius(50%);
			margin-bottom: 14px;
		}
	}

	span{
		display: inline-block;
		width: 36px;
		@include border-radius(7px 0 0 7px);
        @include transition-duration(.4s);
        border-bottom: 3px solid #1B7E61;
		background-color: #22ad84;


	}
	span + div{
		margin-left: 36px;
	}
	#name{
		height: 33px;
		i{
			margin-top: 10px;
			margin-left: 12px;
		}
	}
	#email{
		height: 33px;
		i{
			margin-top: 10px;
			margin-left: 10px;
		}
	}
	#message{
		height: 133px;
		i{ 
			margin-top: 70px;
			margin-left: 12px;
		}
	}

	input[type="text"], input:hover[type="text"], input:focus[type="text"], textarea{
        @include border-radius(0 7px 7px 0);
        border-bottom: 3px solid #999;
		@include box-sizing(border-box);
		width: 100%;
		color: #ccc;
	}
	
	input[type="submit"]{
		height: 49px;
		color: white;
		background-color: #E84C3D;
	    border-bottom: 3px solid #C54033;
	}
	input[type="submit"]:hover{
		background-color: #1e1e1e;
	    border-bottom: 3px solid #1e1e1e;
	} 

	textarea{
		resize: none;
	}
	textarea:focus, textarea:hover{
		color: #999;
	}

	#map{
		width: 100%;
	}
}

/* PAGE STYLES
 * ------------------------------------------------ */
/* Add page styles here */
/* For example: .page-home { background: rgba(0, 0, 0, 0.4); } */

/* MEDIA QUERIES
 * ------------------------------------------------ */
/* Otherwise known as custom responsive code! Write it here! */
@media only screen and (min-width: $baseWidthMQ) and (max-width: 1280px) {
	/* add code here */
}

/* Smaller than standard #{strip-units($baseWidthMQ)} (devices and browsers) */
@media only screen and (max-width: $baseWidthMQ - 1) {
	/* add code here */
	h1{
		line-height: 56px;
		font-size: 61px;
	}

	.landing{

		.container{
			top: 20%;
		}

		.action{
 			margin-top: 21px;
 		}

 		img{
 			margin-top: 21px;
 			width: 192px;
 			height: 192px;
 		}
 	}

 	.about{
 		li{
 			margin: 0 21px;
 		}
 	}

 	.media{
 		.prev, .next{
 			margin-top: -32px;
 		}
 	}
}

/* Tablet Portrait size to standard #{strip-units($baseWidthMQ)} (devices and browsers) */
@media only screen and (min-width: $tabletWidth) and (max-width: $baseWidthMQ - 1) {
	/* add code here */
}

/* All Mobile Sizes (devices and browser) */
@media only screen and (max-width: $tabletWidth - 1) {
	/* add code here */
	h1, h2, h3, h5, p{
		text-align: center;
	}

	.landing{
		.container{
			top: 10%;
		}


 		img{
			display: block;
			margin-left: auto;
			margin-right: auto;
		}
	}

	.media{
		header{
			margin-bottom: -280px;

			h1{
				padding-top: 35px;
			}
		}
	}

	.about{
		article{
			margin-bottom: 21px; 

			a{
				display: inline-block;
				width: 100%;
				text-align: center;
			}
		}
		ul{
	 		li{
	 			width: 49px;
	 			height: 49px;
	 		}

	 		i{
	 			font-size: 2em;
	 		}
	 	}
 	}

 	.contact{

 		.map-holder, #map{
 			display: none;
 		}
 		img{
 			margin-left: auto;
 			margin-right: auto;
 		}
 	}
}

/* Mobile Landscape Size to Tablet Portrait (devices and browsers) */
@media only screen and (min-width: 480px) and (max-width: $tabletWidth - 1) {
	/* add code here */
}

/* Mobile Portrait Size to Mobile Landscape Size (devices and browsers) */
@media only screen and (max-width: 479px) {
	/* add code here */

	h1{
		line-height: 42px;
		font-size: 47px;
	}

	.landing, .about, .contact{
		padding: 35px 0;
	}

	.landing{
		height: auto;
		background: rgba(82, 54, 69, 1);
		
		.container{
			top: 0%;
		}

		header{
			margin-top: 28px;
		}
		
		img{
			width: 128px;
			height: 128px;
		}

		video{
			display: none;
		}
	}
	.about{
		ul{
	 		li{
	 			width: 42px;
	 			height: 42px;
	 			margin: 0 14px;
	 		}

	 		i{
	 			font-size: 1.5em;
	 		}
	 	}

	 	article{
	 		a{
	 			width: 100%;
	 			text-align: center;
	 		}
	 	}
 	}
}
